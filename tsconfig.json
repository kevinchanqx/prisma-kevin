{
  "compilerOptions": {
    "allowUnreachableCode": false,
    "strict": true,
    "noImplicitOverride": true,
    "noUncheckedIndexedAccess": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noEmit": true,
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
  }
}
